{
  "name": "vscode-webview-react-boilerplate",
  "displayName": "VSCode webview react boilerplate",
  "description": "Boilerplate for developing webview using react in VSCode extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "vscode",
    "vscode-extension",
    "vscode-extension-boilerplate",
    "boilerplate"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/src/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vscodeforces-sidebar-view",
          "title": "VSCodeforces",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "vscodeforces-sidebar-view": [
        {
          "type": "webview",
          "id": "vscodeforces-sidebar",
          "name": "VSCodeforces",
          "icon": "media/icon.svg",
          "contextualTitle": "Sample"
        }
      ]
    },
    "commands": [],
    "configuration": {
      "title": "VSCodeforces",
      "properties": {
        "VSCodeforces.userKey": {
          "type": "string",
          "default": ""
        },
        "VSCodeforces.userSecret": {
          "type": "string",
          "default": ""
        },
        "VSCodeforces.showOnlyOnlineFriends": {
          "type": "boolean",
          "default": false,
          "enum": [
            false,
            true
          ],
          "enumDescriptions": [
            "Fetch all friends information",
            "Fetch friends information that are online"
          ]
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm-run-all compile:*",
    "compile:extension": "tsc -p ./",
    "compile:view": "webpack --mode development",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc -watch -p ./",
    "watch:view": "webpack --watch --mode development",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "sha512-crypt-ts": "^0.1.27",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.15",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1",
    "webpack": "^5.21.1",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.26",
    "js-sha512": "^0.8.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.3.6"
  }
}
